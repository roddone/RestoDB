{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    //"RestoDB": "Host=localhost;Database=test-db;Username=postgres;Password=postgres",
    "RestoDB": "Data Source=..\\sqlite-sample.db;"
  },
  "LimitTo": null,
  "DbProvider": "Sqlite",
  "EnableSwagger": true,
  "ApiSegment": "api-test",
  "Authentication": {
    "Enabled": false,
    "Issuer": "https://toto",
    "Audience": "toto",
    "ValidateLifetime": true,
    "ValidateIssuer": false,
    "ValidateIssuerSigningKey": false
  },
  "Cache": {
    "Enabled": true,
    "DurationInSeconds": 5,
    "Absolute": true
  },
  "Cors": {
    "Enabled": false,
    "AllowedOrigins": null,
    "AllowedMethods": null,
    "AllowedHeaders": null
  },
  "RateLimiter": {
    "Enabled": true,
    "PermitLimit": 10,
    "WindowInSeconds": 60,
    "QueueLimit":  0
  }
}
